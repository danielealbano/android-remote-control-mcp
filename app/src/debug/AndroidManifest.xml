<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
      Debug-only manifest overlay.
      Registers E2EConfigReceiver for test configuration injection via adb shell.
      This receiver is NOT included in release builds.
      exported=true is required because adb shell runs as a different user (shell)
      and Android 14+ blocks broadcasts to non-exported receivers from external callers.
    -->
    <application>
        <receiver
            android:name=".debug.E2EConfigReceiver"
            android:exported="true">
        </receiver>
    </application>

</manifest>
